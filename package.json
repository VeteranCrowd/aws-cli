{
  "name": "@veterancrowd/aws-cli",
  "bin": {
    "vc": "bin/vc/index.js"
  },
  "version": "4.3.6",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veterancrowd/aws-cli"
  },
  "author": "Jason G. Williscroft",
  "bugs": {
    "url": "https://github.com/veterancrowd/aws-cli/issues"
  },
  "description": "An ES6 NPM package project template featuring a CLI, test support, automated API docs, release management & more!",
  "homepage": "https://github.com/veterancrowd/aws-cli#readme",
  "keywords": [
    "aws"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-amplify": "^3.370.0",
    "@aws-sdk/client-api-gateway": "^3.370.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.370.0",
    "@aws-sdk/client-dynamodb": "^3.386.0",
    "@inquirer/confirm": "^2.0.8",
    "@karmaniverous/edge-logger": "^1.3.0",
    "@karmaniverous/get-dotenv": "^3.1.12",
    "@veterancrowd/wrapped-secrets-manager-client": "^0.3.4",
    "@veterancrowd/wrapped-sqs-client": "^0.3.4",
    "commander": "^11.0.0",
    "depcheck": "^1.4.3",
    "depcheck-parser-babel": "^3.0.34",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "git-branch": "^2.0.1",
    "globby": "^13.2.2",
    "lodash": "^4.17.21",
    "pkg-dir": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "auto-changelog": "^2.4.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.3.7",
    "concat-md": "^0.5.1",
    "crlf": "^1.1.1",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdoc-to-markdown": "^8.0.0",
    "lefthook": "^1.4.6",
    "mocha": "^10.2.0",
    "release-it": "^16.1.3"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./dist/default/lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "mocha": {
    "exclude": [
      "./dist/**",
      "./node_modules/**"
    ],
    "file": "./test/setup.js",
    "require": [
      "@babel/register"
    ],
    "spec": "./**/*.test.!(*.*)",
    "timeout": 60000
  },
  "release-it": {
    "git": {
      "changelog": "npx auto-changelog --stdout --commit-limit false -u --template https://raw.githubusercontent.com/release-it/release-it/main/templates/changelog-compact.hbs",
      "requireBranch": "dev",
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "hooks": {
      "after:init": [
        "nr lint",
        "nr test",
        "nr build",
        "nr doc"
      ],
      "after:release": [
        "git switch -c release/${version}",
        "git push -u origin release/${version}",
        "git switch dev"
      ]
    },
    "npm": {
      "publish": true
    }
  },
  "scripts": {
    "build": "babel lib -d dist/default/lib --delete-dir-on-start --config-file ./dist/default/.babelrc",
    "doc": "jsdoc2md -c doc/jsdoc.config.json -f lib/**/*.* -t doc/api-template.hbs > doc/3-api.jsdoc2.md && concat-md doc --hide-anchor-links > README.md && crlf --set=CRLF README.md",
    "lint": "eslint lib/** bin/**",
    "release": "node ./bin/vc release-it",
    "test": "node ./bin/vc mocha"
  },
  "type": "module"
}

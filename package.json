{
  "name": "@veterancrowd/aws-cli",
  "bin": {
    "mycli": "bin/vcaws/index.js"
  },
  "version": "2.0.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veterancrowd/aws-cli"
  },
  "author": "Jason G. Williscroft",
  "bugs": {
    "url": "https://github.com/veterancrowd/aws-cli/issues"
  },
  "description": "An ES6 NPM package project template featuring a CLI, test support, automated API docs, release management & more!",
  "homepage": "https://github.com/veterancrowd/aws-cli#readme",
  "keywords": [
    "aws"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-amplify": "^3.303.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.303.0",
    "@karmaniverous/edge-logger": "^1.2.0",
    "@karmaniverous/get-dotenv": "^1.2.0",
    "@veterancrowd/wrapped-secrets-manager-client": "^0.2.0",
    "@veterancrowd/wrapped-sqs-client": "^0.2.2",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "openapi-client-axios-typegen": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@types/node": "^18.15.11",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.3.7",
    "concat-md": "^0.5.1",
    "eslint": "^8.37.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.1.1",
    "eslint-plugin-mocha": "^10.1.0",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.7",
    "release-it": "^15.10.0"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./dist/default/lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "mocha": {
    "exclude": [
      "./dist/**",
      "./node_modules/**"
    ],
    "file": "./test/setup.js",
    "require": [
      "@babel/register"
    ],
    "spec": "./**/*.test.!(*.*)"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "scripts": {
    "build": "babel lib -d dist/default/lib --delete-dir-on-start --config-file ./dist/default/.babelrc",
    "cli": "node --no-warnings ./bin/vcaws",
    "doc": "jsdoc2md -c doc/jsdoc.config.json -f lib/**/*.* -t doc/api-template.hbs > doc/3-api.jsdoc2.md && concat-md doc --hide-anchor-links > README.md",
    "lint": "eslint lib/** bin/**",
    "package": "npm run lint && npm run test && npm run build && npm run doc",
    "release": "npm run package && getdotenv -- release-it",
    "test": "getdotenv -c \"mocha\" -p ./ ./env -d dev -y ./env/dynamic.js"
  },
  "type": "module"
}
